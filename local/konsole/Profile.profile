[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=Catppuccin
Font=Iosevka Nerd Font,14,-1,5,50,0,0,0,0,0
UseFontLineChararacters=true

[Cursor Options]
CustomCursorColor=205,214,244
UseCustomCursorColor=true

[General]
Command=/bin/zsh --login
Name=Profile
Parent=FALLBACK/
TerminalCenter=false
TerminalMargin=20

[Interaction Options]
MiddleClickPasteMode=1
UnderlineFilesEnabled=true

[Keyboard]
KeyBindings=default

[Scrolling]
HistoryMode=1
HistorySize=2000
ScrollBarPosition=1
ScrollFullPage=true

[Terminal Features]
BlinkingCursorEnabled=true
